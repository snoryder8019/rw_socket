</select>
  <div id="usersList">
    <% users.forEach(user => { %>
      <div class="user-item" 
           data-name="<%= (user.firstName || '').toLowerCase() %>" 
           data-email="<%= (user.email || '').toLowerCase() %>">
        <p><strong>Name:</strong> <%= user.firstName || 'No name provided' %></p>
        <p><strong>Email:</strong> <%= user.email || 'No email provided' %></p>
      </div>
    <% }) %>
  </div>


</body>
