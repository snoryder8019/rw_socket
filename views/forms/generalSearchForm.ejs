<!DOCTYPE html>
<html>
<head>
  <title>Search Users</title>
  <style>

  </style>
</head>
<body>
  <h1>Search Users</h1>
  <input type="text" id="searchInput" placeholder="Search by name or email..." onkeyup="filterUsers()">

  <div id="usersList">
    <% users.forEach(user => { %>
      <div class="user-item" 
           data-name="<%= (user.firstName || '').toLowerCase() %>" 
           data-email="<%= (user.email || '').toLowerCase() %>">
        <p> <%= user.firstName || 'No name provided' %>-<%= user.email || 'No email provided' %>
        <button>:</button></p>
      </div>
    <% }) %>
  </div>


</body>
</html>
