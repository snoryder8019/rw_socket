<div id="gameMenu">
    <h1>Game Menu</h1>
    <p>Welcome, <%= user.firstName %>!</p>
  
    <p>Session ID: <%= session._id %></p>

    <div id="liveGameInfo">
        <!-- This will be populated with live game info via Socket.IO -->
        <p>Waiting for players to join...</p>
    </div>

    <div id="playerList">
        players
        <!-- Player list will be dynamically updated via Socket.IO -->
    </div>

    <button class="gameManagers" button-url="games/gameSessions/ready/<%=user._id%>">ready to play</button></div>
    <button class="gameManagers" button-url="games/gameSessions/exit/<%=session._id%>">exit party</button></div>
    <button id="startGameButton">StartGame</button>
