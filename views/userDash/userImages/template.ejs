<div class="adminWindow">
    <div class="adminStatsRow">
      <% documents.forEach(userImage => { %>
        <div class="adminStat">
          <p><%= userImage.altText %></p>
          <img alt="User Image" src="<%= userImage.imageUrl %>">
          <button class="collapsible">view: <span class="carrot">&#9654;</span></button>
          <div class="content" data-url="/userImages/renderEditForm/<%= userImage._id %>"></div>
          <h3>add multiple images to user image slides</h3>
          <form action="/userImages/<%=userImage._id%>/upload-images" method="post" enctype="multipart/form-data">
            <input type="file" name="images" multiple>
            <button type="submit">Upload</button>
          </form>
        </div>
      <% }) %>
    </div>
  </div>
  